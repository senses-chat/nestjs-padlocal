{
  "name": "@senses-chat/padlocal-module",
  "version": "0.1.0",
  "description": "Padlocal Module",
  "author": "Simon Liang <simon@x-tech.io>",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "proto:generate": "proto-loader-gen-types --keepCase --longs=Number --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=src/generated padlocal.proto",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "format": "prettier --write",
    "lint": "eslint --fix"
  },
  "dependencies": {
    "@nestjs/bullmq": "^1.1.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/common": "^8.3.0",
    "@nestjs/core": "^8.3.0",
    "@senses-chat/padlocal-db": "0.1.0",
    "class-transformer": "^0.5.1",
    "date-fns": "^2.28.0",
    "fast-xml-parser": "^4.0.12",
    "he": "^1.2.0",
    "padlocal-client-ts": "^0.5.3",
    "rxjs": "^7.4.0"
  },
  "peerDependencies": {
    "bullmq": "^3.10.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/he": "^1.1.2",
    "rimraf": "^3.0.0"
  },
  "volta": {
    "node": "16.13.1",
    "yarn": "1.22.17"
  }
}
